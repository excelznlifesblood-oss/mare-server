{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "MareSynchronosStaticFilesServer": "Debug",
      "MareSynchronosShared": "Debug",
      "System.IO": "Information",
    },
    "File": {
      "BasePath": "logs",
      "FileAccessMode": "KeepOpenAndAutoFlush",
      "FileEncodingName": "utf-8",
      "DateFormat": "yyyMMdd",
      "MaxFileSize": 104857600,
      "Files": [
        {
          "Path": "<date:yyyy>/<date:MM>/<date:dd>/mare-<date:HH>-<counter:0000>.log"
        }
      ]
    }
  },
  "MareSynchronos": {
    "DbContextPoolSize": 512,
    "ShardName": "Files",
    "MetricsPort": 6350,
    "ForcedDeletionOfFilesAfterHours": -1,
    "CacheSizeHardLimitInGiB": 94,
    "UnusedFileRetentionPeriodInDays": 14,
    "CacheDirectory": "/marecache/",
    "RemoteCacheSourceUri": "",
    "MainServerAddress": "http://localhost:5002/",
    "RedisConnectionString": "localhost,6379",
    "MainFileServerAddress": "",
    "Jwt": "teststringteststringteststringteststringteststringteststringteststringteststringteststringteststring",
    "UseColdStorage": false,
    "IsDistributionNode": true,
    "CdnFullUrl": "https://sync.nm-shonin.com/cache/",
    "ColdStorageDirectory": "",
    "ColdStorageSizeHardLimitInGiB": -1,
    "ColdStorageUnusedFileRetentionPeriodInDays": 30,
    "DownloadQueueSize": 100,
    "DownloadTimeoutSeconds": 120,
    "DownloadQueueReleaseSeconds": 15,
    "DownloadQueueClearLimit": 15000,
    "CleanupCheckInMinutes": 15,
    "SpeedTestHoursRateLimit": 0.5
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://+:5003"
      }
    }
  },
  "IpRateLimiting": {},
  "IPRateLimitPolicies": {}
}